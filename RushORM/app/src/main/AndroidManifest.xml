<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="co.uk.rushexample" >

    <application
        android:allowBackup="true"
        android:name="co.uk.rushexample.SampleApplication"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="co.uk.rushexample.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- These tags are included by having RushAndroid as a module dependency they are required with importing via gradle
        <meta-data android:name="Rush_db_version" android:value="1" />
        <meta-data android:name="Rush_db_name" android:value="rush.db" />
        <meta-data android:name="Rush_debug" android:value="true" />
        <meta-data android:name="Rush_log" android:value="true" /> -->

    </application>

</manifest>
